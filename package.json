{
  "name": "cephalon-tobran-backend",
  "version": "0.3.3",
  "description": "Manage the behind-the-scenes aspects of Cephalon Tobran.",
  "main": "\\src\\main.ts",
  "keywords": [
    "warframe"
  ],
  "repository": "git@github.com:CephalonTobran/backend.git",
  "author": "EdRands <erands@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12.*"
  },
  "scripts": {
    "import": "ts-node src\\import.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "commit": "git-cz",
    "test": "jest",
    "test:watch": "majestic",
    "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
    "lint:ci": "eslint \"**/*.{js,ts}\" --color",
    "release": "semantic-release"
  },
  "dependencies": {
    "@firebase/app-types": "^0.6.0",
    "@types/dompurify": "^2.0.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.4",
    "@types/semver": "^7.1.0",
    "@types/validator": "^13.0.0",
    "@types/yargs": "^15.0.4",
    "chalk": "^4.0.0",
    "dompurify": "^2.0.10",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.11.0",
    "jsdom": "^16.2.2",
    "lodash": "^4.17.15",
    "semver": "^7.3.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "validator": "^13.0.0",
    "warframe-items": "^1.891.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "commitizen": "^4.0.5",
    "conventional-changelog-conventionalcommits": "^4.2.3",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-tsdoc": "^0.2.4",
    "expect-more-jest": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^25.5.2",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "jest-junit": "^10.0.0",
    "majestic": "^1.6.2",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7",
    "ts-jest": "^25.4.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
